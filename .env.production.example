# Production Environment Variables for Netlify
# Copy these to your Netlify dashboard under Site Settings > Environment Variables
# Use VITE_ prefix for Netlify deployment

# AI Configuration
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Authentication (Clerk)
VITE_CLERK_PUBLISHABLE_KEY=your_clerk_publishable_key_here

# Payment Providers - LIVE KEYS
# Razorpay (Indian payments)
VITE_RAZORPAY_KEY_ID=your_razorpay_key_id_here
VITE_RAZORPAY_KEY_SECRET=your_razorpay_secret_key_here

# PayPal (International payments)
VITE_PAYPAL_CLIENT_ID=your_paypal_client_id_here
VITE_PAYPAL_CLIENT_SECRET=your_paypal_client_secret_here
VITE_PAYPAL_ENVIRONMENT=production